#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/liujiang/Desktop/myproject/jiaoshoujia/examples/app/node_modules/.pnpm/liu-cli@1.0.0/node_modules/liu-cli/node_modules:/Users/liujiang/Desktop/myproject/jiaoshoujia/examples/app/node_modules/.pnpm/liu-cli@1.0.0/node_modules:/Users/liujiang/Desktop/myproject/jiaoshoujia/examples/app/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/liujiang/Desktop/myproject/jiaoshoujia/examples/app/node_modules/.pnpm/liu-cli@1.0.0/node_modules/liu-cli/node_modules:/Users/liujiang/Desktop/myproject/jiaoshoujia/examples/app/node_modules/.pnpm/liu-cli@1.0.0/node_modules:/Users/liujiang/Desktop/myproject/jiaoshoujia/examples/app/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir//user/bin/env" ]; then
  exec "$basedir//user/bin/env"  node "$basedir/../liu-cli/index.js" "$@"
else
  exec /user/bin/env  node "$basedir/../liu-cli/index.js" "$@"
fi
